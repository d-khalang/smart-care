      ],
      "responses": {
        "200": {
          "description": "Successful response",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "success": {
                    "type": "boolean",
                    "example": true
                  },
                  "content": {
                    "type": "object",
                    "oneOf": [
                      {
                        "$ref": "#/definitions/broker"
                      },
                      {
                        "$ref": "#/definitions/template"
                      }
                    ]
                  },
                  "status": {
                    "type": "integer",
                    "example": 200
                  }
                },
                "required": [
                  "success",
                  "content",
                  "status"
                ]
              }
            }
          }
        },
        "400": {
          "description": "Invalid subpath",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/error"
              }
            }
          }
        },
        "404": {
          "description": "Not found",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/error"
              }
            }
          }
        },
        "500": {
          "description": "Server error",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/error"
              }
            }
          }
        }
      }
    },
